        },
        {
          "command": "edkrepo.menus.log",
          "group": "group1"
        },
        {
          "command": "edkrepo.menus.update",
          "group": "group1"
        },
        {
          "command": "edkrpeo.menus.sparse",
          "group": "group1"
        }
      ]
    }
